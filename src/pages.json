{
  "easycom": {
    "autoscan": true,
    "custom": {
      "fui-(.*)": "firstui-uni/firstui/fui-$1/fui-$1.vue",
      "^u--(.*)": "uview-plus/components/u-$1/u-$1.vue",
      "^up-(.*)": "uview-plus/components/u-$1/u-$1.vue",
      "^u-([^-].*)": "uview-plus/components/u-$1/u-$1.vue"
    }
  },
  "pages": [
    //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
    {
      "path": "pages/home/home",
      "style": {
        "navigationBarTitleText": "首页"
      }
    },
    {
      "path": "pages/logIn/logIn",
      "style": {
        "navigationBarTitleText": "登录"
      }
    },
    {
      "path": "pages/myHome/myHome",
      "style": {
        "navigationBarTitleText": "我的"
      }
    },
    {
      "path": "pages/player/player",
      "style": {
        "navigationBarTitleText": "播放器"
      }
    }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "uni-app",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8"
  },
  "subPackages": [
    {
      "root": "playbackPlatform",
      "pages": [
        {
          "path": "Platform/Platform",
          "style": {
            "navigationBarTitleText": "Platform"
          }
        }
      ]
    }
  ],
  "tabBar": {
    // "custom": true,
    "color": "#000000",
    "selectedColor": "#dd1e1e",
    "backgroundColor": "#ffffff",
    "list": [
      {
        "pagePath": "pages/home/home",
        "text": "首页",
        "color": "#000000",
        "selectedColor": "#dd1e1e",
        "iconPath": "static/首页b.png",
        "selectedIconPath": "static/首页a.png"
      },
      {
        "pagePath": "pages/player/player",
        "text": "播放器",
        "color": "#000000",
        "selectedColor": "#dd1e1e",
        "iconPath": "static/音乐b.png",
        "selectedIconPath": "static/音乐a.png"
      },
      {
        "pagePath": "pages/myHome/myHome",
        "text": "我的",
        "color": "#000000",
        "selectedColor": "#dd1e1e",
        "iconPath": "static/我的b.png",
        "selectedIconPath": "static/我的a.png"
      }
    ]
  },
  "usingComponents": {}
}
